//Accept data in 3*3 matrix and print it.
#include <stdio.h>
int main()
{
    int i, j, a[3][3];
    printf("Enter the 9 elements: ");
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
            scanf("%d", &a[i][j]);
    }
    printf("The 3x3 matrix is \n");
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
            printf("%d\t", a[i][j]);
        printf("\n");
    }
    return 0;
}
