//Accept data in 3*3 matrix and print column wise sum.
#include<stdio.h>
int main()
{
    int i,j,s;
	int arr[3][3], r[3];
	printf("Enter the 9 elements: ");
	for(i=0;i<3;i++)
	{
	    for(j=0; j<3; j++)
	        scanf("%d",&arr[i][j]);
	}
	printf("The sum of the column elements are:");
	for(j=0;j<3;j++)
	{
	    s=0;
	    for(i=0; i<3; i++)
	        s+=arr[i][j];
	    r[i]=s;
	    printf("%d  ",r[i]);
	}
	return 0;
}
