//Print the element from a given index number
#include<stdio.h>
int main()
{
    int in, n, i, arr[20];
    printf("Enter the numbers of elements in array: ");
    scanf("%d", &n);
    printf("Enter the elements of the array: ");
    for(i=0; i<n; i++)
        scanf("%d", &arr[i]);
    printf("Enter a valid index number: ");
    scanf("%d", &in);
    for(i=in; i<n; i++)
        printf("%d  ", arr[i]);
    return 0;
}
