//Find the highest number in the array
#include<stdio.h>
int main()
{
    int n, i, max=0, arr[20];
    printf("Enter the numbers of elements in array: ");
    scanf("%d", &n);
    printf("Enter the elements of the array: ");
    scanf("%d", &arr[0]);
    max=arr[0];
    for(i=1; i<n; i++)
    {
        scanf("%d", &arr[i]);
        if(max<arr[i])
            max=arr[i];
    }
    printf("The highest numbers is %d", max);
    return 0;
}
