//Cube of numbers in an array
#include<stdio.h>
int main()
{
    int n, i, arr[20], cube[20];
    printf("Enter the numbers of elements in array: ");
    scanf("%d", &n);
    printf("Enter the elements of the array: ");
    for(i=0; i<n; i++)
    {
        scanf("%d", &arr[i]);
        cube[i]=arr[i]*arr[i]*arr[i];
        printf("The cube of %d = %d\n",arr[i], cube[i]);
    }return 0;
}
