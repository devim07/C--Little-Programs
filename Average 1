//average of elements in an array
#include<stdio.h>
int main()
{
    int n, i, arr[20], sum=0;
    printf("Enter the numbers of elements in array: ");
    scanf("%d", &n);
    printf("Enter the elements of the array: ");
    for(i=0; i<n; i++)
    {
        scanf("%d", &arr[i]);
        sum+=arr[i];
    }
    printf("The average of the array = %f", sum*1.0/n);
    return 0;
}
