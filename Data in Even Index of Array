//print data of even index
#include<stdio.h>
int main()
{
    int n, i, arr[20], sum=0;
    printf("Enter the numbers of elements in array: ");
    scanf("%d", &n);
    printf("Enter the elements of the array: ");
    for(i=0; i<n; i++)
        scanf("%d", &arr[i]);
    printf("The data in the even index of the arry are\n");
    for(i=0; i<n; i+=2)
        printf("%d\t", arr[i]);
}
