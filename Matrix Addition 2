//Accept data in 3*3 matrix and do the sum of all elementS
#include <stdio.h>
int main()
{
    int i, j, a[3][3], sum=0;
    printf("Enter the 9 elements: ");
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
        {
            scanf("%d", &a[i][j]);
            sum+=a[i][j];
        }
    }
    printf("The sum of all elements of the matrix = %d", sum);
    return 0;
}
